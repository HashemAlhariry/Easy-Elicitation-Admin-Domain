extends layout
block content
    div(class="page-wrapper bg-light p-t-100 p-b-50")
        div(class="wrapper wrapper--w900")
            div(class="card card-6")
                div(class="card-heading")
                    h2(class="title") Admins
            div(class="card-body")
                table(id="tables")
                    th name
                    th email
                    th delete
                    for i in result
                        tr
                        td #{i.name}
                        td #{i.email}
                        td 
                            a(href="/deleteadmin?adminid="+i.systemAdminId,class="btn btn--radius-2 btn--blue-2") delete
                